<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Kernel>
        <Debug>true</Debug>
        <DisplayErrors>1</DisplayErrors>
        <EnableCache>1</EnableCache>
        <Encoding>utf-8</Encoding>
        <ErrorReporting>2047</ErrorReporting>
        <ErrorReportingType>exception</ErrorReportingType>
        <ForceCompression>false</ForceCompression>
        <Locale>ru_RU</Locale>
        <PreferDeflate>0</PreferDeflate>
        <ProfilePath>profile</ProfilePath>
        <StartSession>false</StartSession>
        <Timezone>Europe/Moscow</Timezone>
        <UseOutputBuffering>false</UseOutputBuffering>
    </Kernel>
    <Module>
        <CacheModule>
            <Providers></Providers>
        </CacheModule>
        <CaptchaModule>
            <Providers></Providers>
        </CaptchaModule>
        <DataModule>
            <Providers></Providers>
        </DataModule>
        <InvarModule>
            <SectionKeyName>section</SectionKeyName>
            <SectionKeyTemplate>%section%</SectionKeyTemplate>
            <ActionKeyName>action</ActionKeyName>
            <ActionKeyTemplate>%action%</ActionKeyTemplate>
            <LoaderClass>HfuInvarLoader</LoaderClass>
        </InvarModule>
        <LogModule>
            <Level>255</Level>
            <Providers></Providers>
        </LogModule>
        <SessionModule></SessionModule>
        <TemplateModule></TemplateModule>
        <RedirectModule>
            <WaitTime>300</WaitTime>
            <Template></Template>
        </RedirectModule>
    </Module>
    <Profile>
        <CacheScript>0</CacheScript>
        <CacheStyle>0</CacheStyle>
        <CheckCacheModifyTime>0</CheckCacheModifyTime>
    </Profile>
</Configuration>